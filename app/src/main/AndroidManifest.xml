<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gestibank">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GestiBank.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".client.EspaceClients"
            android:parentActivityName=".Connexion"
            android:label="@string/title_activity_espace_clients"
            android:theme="@style/Theme.GestiBank.NoActionBar"></activity>
        <activity android:name=".admin.AjoutAgents" />
        <activity android:name=".admin.ListeAgents" />
        <activity android:name=".admin.ListeDemandes" />
        <activity android:name=".admin.EspaceAdmin" />
        <activity android:name=".Connexion"
            android:theme="@style/Theme.GestiBank.NoActionBar"/>
        <activity android:name=".CreationCompte" />
        <activity android:name=".Conversion" />
        <activity android:name=".MainActivity"
            android:theme="@style/Theme.GestiBank.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>